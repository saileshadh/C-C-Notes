#include <iostream>
using namespace std;

int main() {
    int a=5, b=6, c=8;
    if(a > b) {
        if(a > c) {
            cout << "a is largest";
        } else {
            cout << "c is largest";
        }
    } else {
        if(b > c) {
            cout << "b is largest";
        } else {
            cout << "c is largest";
        }
    }
    return 0;
}
